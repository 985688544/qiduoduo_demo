    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.14",
    "qs": "^6.11.0",
    "style-resources-loader": "^1.5.0",
    "vite-plugin-mock": "^2.9.6",
    "vue": "^3.2.37",
